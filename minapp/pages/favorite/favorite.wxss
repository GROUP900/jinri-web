.likelist {
min-height:100vh;
background:black;
padding-top:30rpx;
width:74%;
position:relative;
margin:0 auto;
box-sizing:border-box;
}
.hint{
color:#b2b2b2;
font-size:40rpx;
font-weight:500;
position:absolute;
left:50%;
top:50%;
transform:translate(-50%,-50%);
text-align:center;
width:80%;
}
.hint icon{
display:block;
margin-bottom:30rpx;
text-align:center;
}

.hint .small{
font-weight:normal;
margin-top:25rpx;
font-size:28rpx;
display:block;
}

.likeblock{
border-radius:16rpx;
background:black;
z-index:500;
transition:.1s;
margin-bottom:30rpx;
height:0;
position:relative;
padding-bottom:56.25%;
}

.likeblock>image{
position:absolute;
width:100%;
height:100%;
border-radius:16rpx;
z-index:1;
}

.likeblock>.shader{
z-index:2;
content:'';
height:100%;
width:100%;
opacity:0.4;
position:absolute;
border-radius:16rpx;
}

.likeblock>.link{
z-index:2500;
width:100%;
height:100%;
position:absolute;
}
.likeblock>button{
padding:0;
margin:0;
z-index:5000;
width:44rpx;
height:56rpx;
right:32rpx;
top:32rpx;
position:absolute;
background:transparent;
border-radius:0;
background-repeat: no-repeat;
background-size: contain;
background-image: url(data:image/svg+xml;base64,);
}

.likeblock>text{
font-family:din;
position:absolute;
z-index:3;
left:32rpx;
top:32rpx;
color:white;
font-size:110rpx;
line-height:80rpx;
}
.likeblock>text .small{
display:block;
font-size:90rpx;
}

.likelist>view{
position:relative;
}

.delbtn {
background:#F44336;
color:black;
position:absolute;
right:0;
top:0;
height:100%;
width:100px;
border-radius:16rpx;
}

.delbtn>text{
font-size:34rpx;
position:absolute;
top:50%;
left:50%;
transform:translate(-50%,-50%);
font-weight:bolder;
}
