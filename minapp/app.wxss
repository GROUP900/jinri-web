page {
background:black;
}
@font-face {
	font-family: 'din';
	src: url(data:application/font-woff;charset=utf-8;base64,) format('woff');
	font-weight: normal;
	font-style: normal;
}

.stage {
overflow-x:hidden;
position:absolute;
background:black;
left:0;
top:0;
width:100%;
height:100%;
}

.stage.day{
height:85%
}

.movablewrapper {
	z-index:500;
  position:absolute;
  width:400%;
  height:889rpx;
  left:50%;
  top:50%;
  transform:translate(-50%,-51%);
	transition:.3s;
}



.infopanel {
position:absolute;
bottom:80rpx;
color:white;
left:50%;
transform:translateX(-50%);
text-align:center;
z-index:5;
width:100%;
opacity:0;
transition:.2s;
}

.infopanel.true{
opacity:1
}

.infopanel rich-text{
letter-spacing:10rpx;
line-height:68rpx;
font-size:28rpx;
}

.infopanel.day {
bottom:180rpx;
}


.infopanel>button {
letter-spacing:0;
margin-top:40rpx;
}

.movablewrapper.true {
margin-top:-550rpx!important;
}
.movablewrapper.moving {
transition:none;
}

.picwrapper {
  width:500rpx;
  perspective:1000px;
	height:0;
}

.picview {
  height:0;
  width:100%;
  position: relative;
  padding-bottom:177.77%;
  background:#333333;
  overflow:hidden;
  border-radius:8px;
  opacity:0;
  transition:1.5s;
}

.picview.active{
  opacity:1;
}


.picview>image {
width:100%;
height:100%;
position:absolute;
overflow:hidden;
}

.picview>image.frontpic {
z-index:300;
}

.picview>image.backpic {
z-index:200;
opacity:0;
}

.date {
z-index:2;
position:absolute;
height:100%;
width:100%;
line-height:80rpx;
}

.date > view {
position:absolute;
top:50%;
transform:translateY(-50%);
font-family: din;
color:white;
font-size:110rpx;
transition:.2s;
opacity:0;
}

.date>view image{
position:absolute;
width:40rpx;
height:164rpx;
top:-9rpx;
}

.date>view image.left{
left:-46rpx;
transform:rotateY(180deg);
}

.date>view image.right{
right:-46rpx;
}

.date .small {
	display: block;
	font-size: 90rpx;
}

.llabel.right {opacity:1;}
.rlabel.left {opacity:1}

.llabel {
left:70rpx;
}
.rlabel {
right:70rpx
}

.nomore {
margin-top:-10rpx;
font-size:68rpx!important;
line-height:74rpx;
right:24rpx;
}

.nomore text .small {
font-size:110rpx;
}


.emutab {
position:absolute;
bottom:0;
left:0;
width:100%;
text-align:center;
z-index:9999;
}
.emutab > image {
width:30px;
height:30px;
display:block;
margin:0 auto;
}

.emutab > text {
color:#4d4d4d;
display:block;
font-size:28rpx;
margin:4rpx 0 3rpx 0;
}

.picdetail {
	display:flex;
	width:500rpx;
	background:linear-gradient(#2f2f2f,#222222);
	border-radius:16rpx;
	margin:0 auto;
	justify-content:space-around;
	height:180rpx;
	align-items:center;
	margin-top:50rpx;
	padding:0 42rpx;
	box-sizing:border-box;
}

.picdetail view {
text-align:center;
font-size:22rpx;
width:100rpx;
}

.picdetail view.active {
opacity:0.4
}

.picdetail view image {
margin:0 auto;
margin-bottom:34rpx;
display:block;
}

.sharebtn {
position:fixed;
bottom:0;
width:100%;
height:14.5%;
transition:.2s;
opacity:0;
}
.sharebtn.active{
opacity:1;
}
.sharebtn>button{
z-index:5000;
width:84rpx;
height:84rpx;
margin:0 auto;
background:white;
border-radius:100%;
background-repeat: no-repeat;
background-size: 32rpx 40rpx;
background-position:center center;
background-image: url(data:image/svg+xml;base64,);
}
